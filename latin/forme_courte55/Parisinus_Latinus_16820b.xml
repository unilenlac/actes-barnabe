		<pb n="41v" /><cb n="2" />
				<head type="initial"><lb n="10" />
			In natale sancti<lb n="11" />
			Barnabe apostoli.
		</head>
			<milestone n="1.1" />
			<p>
				<seg type="lectio" place="inline" hand="manus1">Lectio i.</seg>
				<lb n="12" />
				Redemptor et salua<lb n="13" />tor
				noster dominus Iesus<lb n="14" />
				Christus antequam de<lb n="15" />
				interitu Sathanȩ<lb n="16" />
				triumphans ui<lb n="17" />ctor
				et exultans pen<subst>
					<del rend="dotted">i</del>
					<add place="above" hand="manus1-dubia">e</add>
				</subst>traret re<lb n="18" />gna
				polorum,  sanctam ȩcclesiam quam<lb n="19" />
				pretioso suo sanguine adquisiuit,<lb n="20" />
				per beatos apostolos et apostolicos uiros<lb n="21" />
				immobiliter muniens fundauit,<lb n="22" />
				mirabiliter corroborans<lb n="23" />
				solidauit, scilicet ut<lb n="24" />
				et ipsa inuiolata sem<lb n="25" />per
				gloriosa maneret,<lb n="26" />
				et illorum predicatione<lb n="27" />
